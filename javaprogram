public class flowernonstatic {
	int price=100;
	Boolean b1;
		public static void main(String[] args) {
		System.out.println("PS");
		flowernonstatic f1=new flowernonstatic();
		System.out.println("The Price of one Flower is: "+f1.price);
		System.out.println("The Price of 2 Flower is: "+f1.newflower(2,100));
		System.out.println("The Fragrance Flower is: "+f1.fragrance(true));
		System.out.println("PE");
	}
int newflower(int qty,int p)
{
	return (qty*p);
}

Boolean fragrance(boolean b1)
{
	if (b1==true)
	{
		System.out.println("gud");
		}
	else
	{
		System.out.println("bad:");
	}	

	return b1;
}

}

OUTPUT
PS
The Price of one Flower is: 100
The Price of 2 Flower is: 200
gud
The Fragrance Flower is: true
