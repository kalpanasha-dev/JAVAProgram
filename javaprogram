public class flower {
	static int price=100;
	static Boolean b1=true;
	public static void main(String[] args) {
		System.out.println("PS");
		System.out.println("The Price of one Flower is: "+flower.price);
		flower f1=new flower();
		System.out.println("The Price of 2 Flower is: "+f1.newflower(2,100));
		if (b1==true)
		{
			System.out.println("Fragrance is good");
			}
		else
		{
			System.out.println("Fragrance is bad");
		}	
		System.out.println("PE");
	}
int newflower(int qty,int p)
{
	return (qty*p);
}
}
OUTPUT
PS
The Price of one Flower is: 100
The Price of 2 Flower is: 200
Fragrance is good
