public class ticketdiscount {
	public static void main(String[] args) {
		int age=60;
		if(age<=3)
		{
			System.out.println("U r  child no need to take ticket");
		}
		else if(age>=4 && age<=10)
		{
			System.out.println("Half ticket");
		}
		else if(age>=11 && age<60)
		{
			System.out.println("Full ticket");
		}
		else if(age >=60)
		{
			System.out.println("Senior citizen,provide 10% discount");
		}
	}

}

OUTPUT
Senior citizen,provide 10% discount
