package Jan20;

public class Globalvariable1 {
	static int i=50;
	static int j=20;
	public static void main(String[] args) {
		System.out.println(i);
		System.out.println(j);
        int j=90;
        System.out.println(i);
        test();
        System.out.println(i);
		System.out.println(j);
	}
static int test()
{
	System.out.println(i);
	System.out.println(j);
	i=100;
	j=5;
	System.out.println(i);
	System.out.println(j);
	return i;
}
}

