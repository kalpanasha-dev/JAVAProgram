package Jan20;

public class calculator {
	static int a=2;
	static int b=4;
	public static void main(String[] args) {
		System.out.println("PS");
		System.out.println(a);
		System.out.println(b);
		int num1=10;
		int num2=5;
		Add(a,b);
		Sub(a,b);
		calculator di=new calculator();
		System.out.println("value of divisionis: :"+di.Div(num1,num2));
		System.out.println("value of Product is :"+di.Mult(12,2));
		Rem(a,b);
		System.out.println("PE");
	}
	static void Add(int a,int b)
	{
		int c=a+b;
		System.out.println("value of addition is : "+c);
	}
	static void Sub(int a,int b)
	{
		int c=a-b;
		System.out.println("value of Subtraction is : "+c);
	}
	int Mult(int a,int b)
	{
		return(a*b);
	}
	int Div(int a,int b)
	{
		return (a/b);
	}
	static void Rem(int a,int b)
	{
		int c=a%b;
		System.out.println("value of Reminder is : "+c);
	}
}

OUTPUT
PS
2
4
value of addition is : 6
value of Subtraction is : -2
value of divisionis: :2
value of Product is :24
value of Reminder is : 2
PE
