package AOof2nos;

public class FNLNInitials {
	public static void main(String[] args) {
		String Fname = "Kalpana";
        String initials1 = Fname.substring(0, 1); 
        String Lname = "Sha";
        String initials2 = Lname.substring(0, 1);
        System.out.println ("Initials Are " + Fname+Lname);
        System.out.println ("Initials Are " + initials1+initials2);

	}

}

OUTPUT
Initials Are KalpanaSha
Initials Are KS

package Addition;

public class FNLNInitials1 {
	public static void main(String[] args)
	{
		String name ="Kalpana Narayanaswamy Shanmugasundaram";
		String nameParts[] = name.split(" ");
		char firstInitial = nameParts[0].charAt(0);
		char middleInitial = nameParts[1].charAt(0);
		char lastInitial = nameParts[2].charAt(0);
		System.out.println ("Initials Are " + firstInitial + middleInitial + lastInitial);
	}

}

OUTPUT
Initials Are KNS
package AOof2nos;

public class Arithmeticoperations {

	public static void main(String[] args) {
		int a,b;
		a=2000;
		b=200;
		int sum=a+b;
		int sub=a-b;
		int mult=a*b;
		int div=a%b;
		System.out.println("Value of a is "+a);
		System.out.println("Value of b is "+b);
		System.out.println("addition is "+ sum);
		System.out.println("Subtraction is "+sub);
		System.out.println("Product is "+mult);
		System.out.println("Division is "+div);
	}
}
	
OUTPUT
Value of a is 2000
Value of b is 200
addition is 2200
Subtraction is 1800
Product is 400000
Division is 0
package AOof2nos;

import java.util.Scanner;

public class Arithmetic1
{
	public static void main(String[] args) {
		float a,b;
		System.out.println("Enter values of a,b:");
		Scanner kalp = new Scanner(System.in);
		a=kalp.nextFloat();
		b=kalp.nextFloat();
		float sum=a+b;
		float sub=a-b;
		float mult=a*b;
		float div=a/b;
		System.out.println("Value of a is "+a);
		System.out.println("Value of b is "+b);
		System.out.println("addition is "+ sum);
		System.out.println("Subtraction is "+sub);
		System.out.println("Product is "+mult);
		System.out.println("Division is "+div);

	}

}

OUTPUT
Enter values of a,b:
10.1
15.3
Value of a is 10.1
Value of b is 15.3
addition is 25.400002
Subtraction is -5.2
Product is 154.53001
Division is 0.66013074

