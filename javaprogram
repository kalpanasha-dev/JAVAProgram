package Jan20;

public class switchcase {

	public static void main(String[] args) {
		float testscore=50.1f;
		char grade;
		if(testscore>=90)
		{
			grade='A';
			System.out.println("Grade = "+grade);
		}
		else if(testscore>=75 && testscore<90)
		{
			grade='B';
			System.out.println("Grade = "+grade);
			
		}
		else if(testscore>=50 && testscore<75)
		{
			grade='B';
			System.out.println("Grade = "+grade);
			
		}
		else
		{
			grade='F';
			System.out.println("Grade = "+grade);
			
		}
	}

}

OUTPUT
Grade = B
package Jan20;

public class Switch {
	public static void main(String[] args) {
		int mon=1;
		String monstr;
		switch(mon){
		case 1:monstr="Jan";
		break;
		case 2:monstr="Feb";
		break;
		case 3:monstr="March";
		break;
		case 4:monstr="April";
		break;
		case 5:monstr="May";
		break;
		case 6:monstr="June";
		break;
		case 7:monstr="July";
		break;
		case 8:monstr="August";
		break;
		case 9:monstr="Sep";
		break;
		case 10:monstr="Oct";
		break;
		case 11:monstr="Nov";
		break;
		case 12:monstr="Dec";
		break;
		default:monstr="Invalid month";
		break;
		}
		System.out.println("Month is "+monstr);
	}

}

OUTPUT
Month is Jan
package Jan20;

public class forloop {
	public static void main(String[] args) {
		for(int i=10;i>0;i--)
		{
			System.out.println("Tick "+i);
			
		}
	}

}

OUTPUT:
Tick 10
Tick 9
Tick 8
Tick 7
Tick 6
Tick 5
Tick 4
Tick 3
Tick 2
Tick 1

package Jan20;

public class whileloop {
	public static void main(String[] args) {
		int i=20;
		while(i>10)
			{
			System.out.println("Tick "+i);
			i--;
			}
	}

}

OUTPUT
Tick 20
Tick 19
Tick 18
Tick 17
Tick 16
Tick 15
Tick 14
Tick 13
Tick 12
Tick 11

