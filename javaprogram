public class dowhile {
	public static void main(String[] args) {
		int i=10;
		System.out.println("PS");
		do{
			System.out.println(i);
			i++;
			if(i%2==0)
			{
				continue;
			}
			else{
				i++;
			}
		}while(i<50);
		
		System.out.println("PE");
	}

}

