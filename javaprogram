package Jan18;

public class swap {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a,b,c;
		a=10;
		b=20;
		System.out.println("Before swap");
		System.out.println(a);
		System.out.println(b);
		c=b;
		b=a;
		a=c;
		System.out.println("After swap");
		System.out.println(a);
		System.out.println(b);
		
	}

}

OUTPUT
Before swap
10
20
After swap
20
10

package Jan18;

public class Swapwithout3variable {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a,b;
		a=10;
		b=20;
		System.out.println("Before swap");
		System.out.println(a);
		System.out.println(b);
		a=a+b;
		b=a-b;
		a=a-b;
		System.out.println("After swap");
		System.out.println(a);
		System.out.println(b);
		
	}

}
OUTPUT
Before swap
10
20
After swap
20
10
package Jan18;

public class Grademarks {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int grade;
		grade=49;
		if(grade>=90)
		{
			System.out.println("Distinction,Grade A");
		}
		else if(grade>=70 && grade <90)
		{
			System.out.println("Pass,Grade B");
		}	
		else if(grade>=41 && grade <70)
		{
			System.out.println("Pass,Grade C");
		}
		else if(grade<=40)
		{
			System.out.println("FAIL");
		}
	}

}

OUTPUT
Pass,Grade C
