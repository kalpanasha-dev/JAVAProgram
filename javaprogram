package Jan20;

public class Perimeternonstatic {
    //int r=100;
	public static void main(String[] args) {
		System.out.println("PS");
		int r=100;
		Perimeternonstatic PNS=new Perimeternonstatic();
		System.out.println("Perimeter of circle :"+PNS.pericircle(r));
		System.out.println("Area of circle :"+PNS.Areacircle(r));
		System.out.println("PE");
			}
double pericircle(int r)
{
	double pc=2*Math.PI*r;
	return pc;
}

double Areacircle(int r)
{
	double AC=Math.PI*r*r;
	return AC;
}
}

OUTPUT
PS
Perimeter of circle :628.3185307179587
Area of circle :31415.926535897932
PE

package Jan20;

public class Perimeter {
static int r=100;
static int l=20;
static int b=30;
static int w=5;
	public static void main(String[] args) {
		System.out.println("PS");
		System.out.println("Perimeter of circle is:" +Perimeter.Pericircle(r));
		System.out.println("Perimeter of circle is:" +Perimeter.Areacircle(r));
		Arearectangle(l,b);
		//int w=4;
		//int l=5;
		Perimeterrectangle(l, w);
		System.out.println("PE");
	}
		static double Pericircle(int r)
		{
		double PC=2*Math.PI*r;
		return PC;
		}
		
		static double Areacircle(int r)
		{
		double Area=Math.PI*r*r;
		return Area;
		}
		static void Arearectangle(int l,int b)
		{
		int Ar=l*b;
		System.out.println("Area of rectangle is:"+Ar);
		}
		static void Perimeterrectangle(int l,int w)
		{
		int Pr=2*(l+w);
		System.out.println("Perimeter of rectangle is:"+Pr);
		}
}

OUTPUT
PS
Perimeter of circle is:628.3185307179587
Perimeter of circle is:31415.926535897932
Area of rectangle is:600
Perimeter of rectangle is:50
PE

