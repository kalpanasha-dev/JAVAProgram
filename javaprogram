package Jan20;

public class reversedno {
	public static void main(String[] args) {
		int num=2001;
		int n=num;
		int temp=0;
		int reversednum=0;
		System.out.println(num+"is a  given number");
		while(n>0)
		{
			temp=n%10;
			n=n/10;
			reversednum=temp;
			
		}			
			if(num==reversednum)
			{
				System.out.println(num+"is a palindrome number");
			}
			else{
				System.out.println(num+"is NOT a palindrome number");
			}
		

	}

}

OUTPUT
2001 is a  given number
2001 is NOT a palindrome number
